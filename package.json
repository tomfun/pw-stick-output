{
  "name": "pw-stick-output",
  "version": "0.0.1",
  "type": "module",
  "description": "stick pipewire nodes to outputs",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.ts",
    "dev": "concurrently \"npm run build:watch\" \"nodemon dist/index.cjs\"",
    "build:watch": "tsc --watch",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "tomfun",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./configHelper": {
      "import": "./dist/configHelper.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "sd-notify": "^2.8.0",
    "systemd-journald": "^2.0.0"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "^2.16.0",
    "@types/eslint": "^9.6.1",
    "@types/lodash": "^4.17.17",
    "@types/minimist": "^1.2.5",
    "@types/node": "^18.19.103",
    "@types/sd-notify": "^2.8.2",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}
